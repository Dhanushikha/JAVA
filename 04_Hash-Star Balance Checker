public class Main{
	public static void main(String[] args) {
		String str = "####***";
		int count = 0;
		int count1 = 0;
		for(int i=0;i<str.length();i++){
		    if(str.charAt(i) == '#'){
		    count++;
		    }
		    else{
		        count1++;
		    }
		}
		if(count1>count){
		    System.out.println(count-count1);
		}
		
		else if(count>count1){
		    System.out.println(count1-count);
		}
		else{
		     System.out.println("valid");
		}
		}
		}
